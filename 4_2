		LIST	P=PIC16F84A
		INCLUDE	"P16F84A.INC"
		
		__CONFIG	_HS_OSC&_WDT_OFF
		
CNT1	EQU		0CH
CNT2	EQU		0DH
CNT3	EQU		0EH

		ORG		0H

MAIN	
		BSF		STATUS,RP0
		CLRF	TRISB
		BCF		STATUS,RP0
MAINLP	
		CLRF	PORTB
		CALL	TIME05	
		MOVLW	0FFH
		MOVWF	PORTB
		CALL	TIME05
		GOTO	MAINLP

TIME0125 
 		MOVLW 	0nnH ;1c
 		MOVWF 	CNT1 ;1c
		NOP
LOOP 
 		CALL 	TIME0125 	;((125+2)*n = 125n +2n ※yはTIMEyyの【合計クロック数】
 		DECFSZ 	CNT1,F 		;1*(n-1)+2*1  = n+1
 		GOTO 	LOOP 		;2(n-1)   =2n-2
 		RETURN  			;2

		END